<!DOCTYPE html>
<html>
  <head>
    <title>PostFlex</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    

    <div id="main-container" class="container">
      <div class="row">
        <nav>
          <li><%= link_to 'New', new_pose_path %></li>
          <li><%= link_to 'Index', poses_path %></li>
        </nav>
      </div>
        <div class="row">
          <div class=".col-md-4">
            <h3>PostFlex</h3>
          </div>
          <div class=".col-md-4">
            <%= yield %>
          </div>
        </div>
    </div>
  	

    

  </body>
</html>
